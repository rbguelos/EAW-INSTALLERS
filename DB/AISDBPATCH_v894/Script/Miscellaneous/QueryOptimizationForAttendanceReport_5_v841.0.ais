    E.GroupsCode
    ,[Description] = E.GroupName
    ,E.LastFirstMIName
    ,E.EmployeeNo
    ,E.CostCenterCode
    ,ETE.WorkDate
    ,[WeekDay] = LEFT(DATENAME(DW,ETE.WorkDate),3)
    ,ETE.LateCount
    ,ETE.Late
    ,ETE.UndertimeCount
    ,ETE.Undertime
    ,ETE.AbsentCount
    ,ETE.Absent
    ,E.ProjectProfileId
    ,EmployeeId = E.ProfileId
    ,E.CompanyId 
/*FROM dbo.vwEmployeeTimeEntry ETE*/
FROM report.fnEmployeeTimeEtnry(0,0,@StartWorkDate,@EndWorkDate,0) ETE

/*PROFILE DETAILS*/
CROSS APPLY
(
   SELECT TOP 1 
	   E.GroupsCode
	   ,E.GroupName
	   ,E.LastFirstMIName
	   ,E.EmployeeNo
	   ,E.CostCenterCode
	   ,ProjectProfileId = E.ProjectId
	   ,E.ProfileId
	   ,E.CompanyId
	   ,E.DepartmentId
   FROM dbo.fnProfileInfo(@CompanyId,@BranchId,0,@OfficeId,0,0,0,@ProjectProfileId,@CostCenterId,@DistrictId,@PositionId,0,0,@UnitId,0,@DivisionId,@GroupId,@TeamId,@LineId,1,ETE.TimeEntryHeaderDetailId)E 
   WHERE E.ProfileId = ETE.ProfileId
)E

/*LEAVE DETAILS*/
OUTER APPLY
(
    SELECT TOP 1 
	   LEAVESTATUSID = 
	   (
		  CASE WHEN ISNULL(@IgnoreLeaves,''false'') = ''true'' THEN 0 
		  ELSE 
		  (
			 CASE WHEN (ISNULL(ETE.Late,0) + ISNULL(ETE.Undertime,0)) > ISNULL(ETELeave.LeaveHours,0) THEN 0 ELSE leavereq.StatusId END
		  ) END
	   )
    FROM attendance.TimeEntryLeaveDetail ETELeave
    INNER JOIN leave.LeaveRequest leavereq 
	   ON leavereq.LeaveRequestId = ETELeave.LeaveRequestId
    WHERE ETELeave.TimeEntryHeaderDetailId = ETE.TimeEntryHeaderDetailId
)LEAVE

/*OB DETAILS*/
OUTER APPLY
(
    SELECT TOP 1 
	   OBSTATUSID = 
	   (
		  CASE WHEN ISNULL(ETE.OBTypeId,0) = 1 THEN 
		  (
			 CASE WHEN ISNULL(@IgnoreOB,''false'') = ''true'' THEN 0 
			 ELSE 0 END
		  ) END
	   )
	   ,TOSTATUSID = 
	   (
		  CASE WHEN ISNULL(ETE.OBTypeId,0) = 2 THEN 
		  (
			 CASE WHEN ISNULL(@IgnoreTimeOff,''false'') = ''true'' THEN 0 
			 ELSE 0 END
		  ) END
	   )
    FROM schedule.OfficialBusiness OB
    WHERE OB.OfficialBusinessId = ETE.OfficialBussinessId 
)OB

WHERE
(
    ((ETE.Late + ETE.Absent + ETE.Undertime) > 0)
    AND
    (ISNULL(LEAVE.LEAVESTATUSID,0) = 0)
    AND 
    (ISNULL(OB.OBSTATUSID,0) = 0)
    AND
    (ISNULL(OB.TOSTATUSID,0) = 0)
    AND
    (
	   CAST(ISNULL(@ShowLate,0) AS DECIMAL(18,2)) = CAST(0 AS DECIMAL(18,2)) OR CAST(ISNULL(ETE.Late,0) AS DECIMAL(18,2)) >= CAST(@ShowLate AS DECIMAL(18,2))
    )
    AND
    (
	   CAST(ISNULL(@ShowUndertime,0) AS DECIMAL(18,2)) = CAST(0 AS DECIMAL(18,2)) OR CAST(ISNULL(ETE.Undertime,0) AS DECIMAL(18,2)) >= CAST(@ShowUndertime AS DECIMAL(18,2))
    )
  /*  AND
    (
	   CAST(ISNULL(@StartWorkDate,''1/1/1900'') AS DATETIME) <= CAST(''1/1/1900'' AS DATETIME) OR
	   (ETE.WorkDate >= CAST(@StartWorkDate AS DATETIME))
    )
    AND
    (   
	   CAST(ISNULL(@EndWorkDate,''1/1/1900'') AS DATETIME) <= CAST(''1/1/1900'' AS DATETIME) OR
	   (ETE.WorkDate <= CAST(@EndWorkDate AS DATETIME))
    ) */
    AND
    ((@EmployeeId = '''') OR (CHARINDEX('''''''' + CAST(ETE.ProfileId as VARCHAR) + '''''''',@EmployeeId) > 0))
    AND
    ((@DepartmentIds = '''') OR (CHARINDEX('''''''' + CAST(E.DepartmentId as VARCHAR) + '''''''',@DepartmentIds) > 0))
)',NULL,1,'QUERY',1,'This report will show the total lates, undertime and absences of employees for a given period, organized by Group.
