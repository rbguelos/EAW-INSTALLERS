   ,E.DivisionCode
   ,E.GroupsCode
   ,E.TeamCode
   ,E.LineCode
   ,E.UnitCode
   ,E.DistrictCode
   ,E.ProjectCode
   ,ETE.AbsentCount
   ,E.CompanyId
   ,LEAVE.LEAVESTATUSID
   ,ETE.TimeEntryHeaderDetailId
/*FROM dbo.vwEmployeeTimeEntry ETE*/
FROM report.fnEmployeeTimeEtnry(0,0,@StartWorkDate,@EndWorkDate,0) ETE
