report.fnEmployeeTimeEtnry
(
0,0
,CASE WHEN CAST(ISNULL(@StartWorkDate,''1/1/1900'') AS DATETIME) = CAST(''1/1/1900'' AS DATETIME) THEN FORMAT(CURRENT_TIMESTAMP,N''01/01/yyyy'') ELSE CAST(@StartWorkDate AS DATETIME) END
,CASE WHEN CAST(ISNULL(@EndWorkDate,''1/1/1900'') AS DATETIME) = CAST(''1/1/1900'' AS DATETIME) THEN FORMAT(CURRENT_TIMESTAMP,N''12/31/yyyy'') ELSE CAST(@EndWorkDate AS DATETIME) END
,0
) 
