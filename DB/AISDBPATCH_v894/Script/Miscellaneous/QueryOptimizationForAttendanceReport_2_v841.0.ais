)E

/*LEAVE DETAILS*/
OUTER APPLY
(
    SELECT TOP 1 
	   LEAVESTATUSID = (CASE WHEN ISNULL(@IgnoreLeaves,''false'') = ''true'' THEN 0 ELSE (CASE WHEN ISNULL(leavereq.StatusId,0) = 2 THEN leavereq.StatusId ELSE 0 END) END)
