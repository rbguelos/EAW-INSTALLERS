			CASE
				WHEN CONVERT(VARCHAR, ETE.WorkDate, 101) <> CONVERT(VARCHAR, ETE.TimeIn, 101) THEN FORMAT(ETE.TimeIn, N''MM/dd/yyyy hh:mm tt'')
				ELSE FORMAT(ETE.TimeIn, N''hh:mm tt'')
			END
			)
		ELSE ''''
	END
	)
	,[TimeOut] =
	(
	CASE
		WHEN CAST(ISNULL(ETE.TimeOut, ''1/1900'') AS DATETIME) > CAST(''1/1/1900'' AS DATETIME) THEN (
			CASE
				WHEN CONVERT(VARCHAR, ETE.WorkDate, 101) <> CONVERT(VARCHAR, ETE.TimeOut, 101) THEN FORMAT(ETE.TimeOut, N''MM/dd/yyyy hh:mm tt'')
				ELSE FORMAT(ETE.TimeOut, N''hh:mm tt'')
			END
			)
		ELSE ''''
	END
	)

	,ActualTimeIn = FORMAT(ETE.ActualTimeIn, N''MM/dd/yyyy hh:mm:ss tt'')
	,ActualTimeOut = FORMAT(ETE.ActualTimeOut, N''MM/dd/yyyy hh:mm:ss tt'')
