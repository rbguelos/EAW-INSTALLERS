ECT     PrincipalBalance      ,PrincipalPayment      ,InterestPayment      ,InterestAmount      ,Amortization      FROM payroll.LoanRequestSchedule    where     LoanRequestId = L.LoanRequestId     ) LRS    WHERE  (      1=1      AND          ([Status] NOT IN (''Draft'',''New''))       AND      (      (CAST(ISNULL(@RequestStartDate,''1/1/1900'') AS DATE) <= CAST(''1/1/1900'' AS DATE))      OR      (CAST(L.DateRequest AS DATE) >= CAST(@RequestStartDate AS DATE))      )      AND      (      (CAST(ISNULL(@RequestEndDate,''1/1/1900'') AS DATE) <= CAST(''1/1/1900'' AS DATE))      OR      (CAST(L.DateRequest AS DATE) <= CAST(@RequestEndDate AS DATE))      )  )        ',NULL,NULL,'QUERY',1,'It will show each loan, and the payments made / payment schedule for each loan.',NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL)
