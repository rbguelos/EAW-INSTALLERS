INSERT INTO security.UserSecurity_Detail([UserSecurityId],[ModuleFormId],[AccessTypeId],[ModuleId])      SELECT      [UserSecurityId] = US.UserSecurityId      ,[ModuleFormId] = F.ModuleFormId      ,[AccessTypeId] = F.AccessTypeId      ,[ModuleId] = F.ModuleId      FROM security.Module_Forms F            CROSS APPLY (SELECT DISTINCT USD.UserSecurityId FROM security.UserSecurity_Detail USD JOIN security.UserSecurity US ON US.UserSecurityId = USD.UserSecurityId WHERE USD.ModuleId IN ('40') AND US.RoleId = 1 AND US.UserSecurityId IS NOT NULL) US      WHERE F.ModuleFormId IN ('10627')      AND US.UserSecurityId NOT IN (SELECT UserSecurityId FROM security.UserSecurity_Detail WHERE ModuleFormId IN ('10627') AND UserSecurityId IS NOT NULL)
