(
   SELECT DISTINCT
   PayrollTypeIds = 
       STUFF((SELECT 
       '','' + CAST(PayrollTypeId AS VARCHAR)
       FROM payroll.PayrollType
       ORDER BY PayrollTypeId
       FOR XML PATH('''')),1,1,'''') 
   FROM payroll.PayrollType
)PT
