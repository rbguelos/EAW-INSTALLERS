    ,BF.FirstMILastName
    ,BF.CompanyId
	,COLS.PayrollTypeId
    /*,BF.PayrollTypeId*/
    ,BF.PayrollDate
    ,[CutOffPeriod] = FORMAT(BF.CutOffStartDate, ''MM/dd/yyyy'') + '' - '' + FORMAT(BF.CutOffEndDate, ''MM/dd/yyyy'')
