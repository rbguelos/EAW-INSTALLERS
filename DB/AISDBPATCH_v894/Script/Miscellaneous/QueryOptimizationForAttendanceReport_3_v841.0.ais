     (
	    CASE
		   WHEN CONVERT(VARCHAR, ETE.WorkDate, 101) <> CONVERT(VARCHAR, ETE.StartTime, 101) THEN /*FORMAT(ETE.StartTime, N''MM/dd/yyyy hh:mm tt'')*/FORMAT(ETE.StartTime,CONCAT(CONFIG.ConfigValue,'' '',''hh:mm tt''))
		   ELSE FORMAT(ETE.StartTime, N''hh:mm tt'')
	    END
     )
	 
	,EndTime =
     (
	    CASE
	 	  WHEN CONVERT(VARCHAR, ETE.WorkDate, 101) <> CONVERT(VARCHAR, ETE.EndTime, 101) THEN /*FORMAT(ETE.EndTime, N''MM/dd/yyyy hh:mm tt'')*/FORMAT(ETE.EndTime,CONCAT(CONFIG.ConfigValue,'' '',''hh:mm tt''))
	 	  ELSE FORMAT(ETE.EndTime, N''hh:mm tt'')
	    END
     )
	,ActualTimeIn = /*FORMAT(ETE.ActualTimeIn, N''MM/dd/yyyy hh:mm:ss tt'')*/FORMAT(ETE.ActualTimeIn,CONCAT(CONFIG.ConfigValue,'' '',''hh:mm tt''))
	,ActualTimeOut = /*FORMAT(ETE.ActualTimeOut, N''MM/dd/yyyy hh:mm:ss tt'')*/FORMAT(ETE.ActualTimeOut,CONCAT(CONFIG.ConfigValue,'' '',''hh:mm tt''))
	,ETE.Late
	,ETE.Undertime
	,E.ProjectProfileId
	,StartWorkDate = ETE.WorkDate
	,EndWorkDate = ETE.WorkDate
	,EmployeeId = ETE.ProfileId
	,E.CompanyId
	,OBSTATUSID = OB.OBSTATUSID
	,TOSTATUSID = OB.TOSTATUSID
	,TYPEID = ETE.OBTypeId
	,LEAVESTATUSID = LEAVE.LEAVESTATUSID /**ISNULL(leave.LEAVESTATUSID,0)**/
	,Sortby = Cols.SortOrder
	,RowId = ROW_NUMBER() OVER(ORDER BY Cols.SortOrder)
/*FROM dbo.vwEmployeeTimeEntry ETE*/
FROM report.fnEmployeeTimeEtnry(0,0,@StartWorkDate,@EndWorkDate,0) ETE
CROSS APPLY 
(
    SELECT TOP 1
		DepartmentName
		,LastFirstMIName
		,EmployeeNo
		,CostCenterCode
		,Position
		,ProjectProfileId
		,CompanyId
		,e.DepartmentId		
		,E.ProfileId		
	FROM dbo.fnProfileInfo(@CompanyId,@BranchId,0,@OfficeId,0,0,0,@ProjectProfileId,@CostCenterId,@DistrictId,0,0,0,@UnitId,0,@DivisionId,@GroupId,@TeamId,@LineId, 1, ETE.TimeEntryHeaderDetailId) e
	WHERE e.ProfileId = ETE.ProfileId
) E

/*LEAVE DETAILS*/
OUTER APPLY
(
    SELECT TOP 1 
	   LEAVESTATUSID = 
	   (
		  CASE WHEN ISNULL(@IgnoreLeaves,''false'') = ''true'' THEN 0 
		  ELSE 
		  (
			 CASE WHEN (ISNULL(ETE.Late,0) + ISNULL(ETE.Undertime,0)) > ISNULL(ETELeave.LeaveHours,0) THEN 0 ELSE leavereq.StatusId END
		  ) END
	   )
    FROM attendance.TimeEntryLeaveDetail ETELeave
    INNER JOIN leave.LeaveRequest leavereq 
	   ON leavereq.LeaveRequestId = ETELeave.LeaveRequestId
    WHERE ETELeave.TimeEntryHeaderDetailId = ETE.TimeEntryHeaderDetailId
)LEAVE

/*OB DETAILS*/
OUTER APPLY
(
    SELECT TOP 1 
	   OBSTATUSID = 
	   (
		  CASE WHEN ISNULL(ETE.OBTypeId,0) = 1 THEN 
		  (
			 CASE WHEN ISNULL(@IgnoreOB,''false'') = ''true'' THEN 0 
			 ELSE 0 END
		  ) END
	   )
	   ,TOSTATUSID = 
	   (
		  CASE WHEN ISNULL(ETE.OBTypeId,0) = 2 THEN 
		  (
			 CASE WHEN ISNULL(@IgnoreTimeOff,''false'') = ''true'' THEN 0 
			 ELSE 0 END
		  ) END
	   )
    FROM schedule.OfficialBusiness OB
    WHERE OB.OfficialBusinessId = ETE.OfficialBussinessId 
)OB

CROSS APPLY
(
	SELECT
		SortOrder = 
		(
			CASE ISNULL(@Sort,0) 
				WHEN 1 THEN E.LastFirstMIName 
				WHEN 2 THEN E.EmployeeNo 
				WHEN 3 THEN E.Position
				WHEN 4 THEN E.DepartmentName
				WHEN 5 THEN E.CostCenterCode
			
			END
		)
)Cols
OUTER APPLY
(
	SELECT TOP 1 
		ConfigValue 
	FROM report.ReportConfiguration 
