	,E.ProjectProfileId
	,EmployeeId = ProfileId 
	,StartWorkDate = WorkDate
	,EndWorkDate = WorkDate
	,E.CompanyId
/*FROM dbo.vwEmployeeTimeEntry ETE*/
FROM report.fnEmployeeTimeEtnry(0,0,@StartWorkDate,@EndWorkDate,0) ETE
CROSS APPLY 
(
    SELECT TOP 1
	   EmployeeNo
	   ,LastFirstMIName
	   ,CostCenterCode
