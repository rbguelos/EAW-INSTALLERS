(
    SELECT TOP 1 
	   e.EmployeeNo
		,e.LastFirstMIName
		,e.CostCenterName
		,e.DepartmentCode
		,e.LineCode
		,e.ManpowerClassificationCode
		,e.CompanyId
		,e.CostCenterGroupDescription
		,e.LineCodeId
		,E.ProjectId
		,E.DepartmentId
    FROM dbo.fnProfileInfo(@CompanyId,@BranchId,0,@OfficeId,0,0,0,@ProjectId,@CostCenterId,@DistrictId,@PositionId,0,0,@UnitId,0,0,0,0,@LineId,1,ETE.TimeEntryHeaderDetailId)E
    WHERE E.ProfileId = ETE.ProfileId
