/**LEAVE DETAILS**/
OUTER APPLY
(
    SELECT TOP 1 
	   leavereq.StatusId 
    FROM attendance.TimeEntryLeaveDetail ETELeave
    INNER JOIN leave.LeaveRequest leavereq 
	   ON leavereq.LeaveRequestId = ETELeave.LeaveRequestId
    WHERE ETELeave.TimeEntryHeaderDetailId = ETE.TimeEntryHeaderDetailId
)LEAVE

/**OB**/
OUTER APPLY
(
    SELECT TOP 1 
	   OB.StatusId 
    FROM schedule.OfficialBusiness OB
    WHERE OB.OfficialBusinessId = ETE.OfficialBussinessId
)OB

WHERE
(
    (ETE.Absent + ETE.AbsentCount > 0 )
    AND
    (ISNULL(OB.StatusId,0) <> 2)
    AND
    (((ISNULL(LEAVE.StatusId,0) <> 2 AND ISNULL(@ShowAsentWithOutLeave,''false'') = ''true'') OR ISNULL(@ShowAsentWithOutLeave,''false'') = ''false''))
    AND
    (
	   CAST(ISNULL(@StartWorkDate,''1/1/1900'') AS DATETIME) <= CAST(''1/1/1900'' AS DATETIME) OR
	   (ETE.WorkDate >= CAST(@StartWorkDate AS DATETIME))
    )
    AND
    (   
	   CAST(ISNULL(@EndWorkDate,''1/1/1900'') AS DATETIME) <= CAST(''1/1/1900'' AS DATETIME) OR
	   (ETE.WorkDate <= CAST(@EndWorkDate AS DATETIME))
    )
    AND
    (ISNULL(@EmployeeId,'''') = '''' OR (CHARINDEX('''''''' + CAST(ETE.ProfileId AS VARCHAR) + '''''''', @EmployeeId) > 0))
    AND
    ((@DepartmentIds = '''') OR (CHARINDEX('''''''' + CAST(E.DepartmentId as VARCHAR) + '''''''',@DepartmentIds) > 0))
)',NULL,1,'QUERY',1,'The Absentee Report will show the employees who were absent for a particular period.','\TimeAndAttendance\AbsenteeReport.png',NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL)
INSERT [report].[Report] ([ReportId],[ReportCode],[ReportName],[ReportFile],[ReportExcelFile],[EmploymentStatus],[TableSPUsed],[SPUsed],[NoOrderBy],[QueryType],[InsertToReportTempTable],[Description],[ImageFile],[SortFields],[Event],[ExportFormat],[CreateId],[Createdate],[LastUpdateId],[LastUpdateDate],[UseExportToExcelAPI],[DetailColStartPos],[DetailRowStartPos],[HeaderRowStartPos],[IncludeColHeader],[InsertNewRowAtRowPos],[Template],[SourceName],[ShowGrandTotals],[RepeatColumnHeaderOnHeaderGroup],[RepeatColumnHeaderOnGroupLevel],[AutoSizeColumn],[MaxRowPerPage],[DisplayReportHeader])VALUES(229,'LatesAndUndertimeReport','Lates and Undertime Report','\Reports\DetailedAttendance\LatesAndUndertimeReport.rpt',NULL,NULL,'SELECT                 
	EmployeeNo = E.EmployeeNo
	,EmployeeName = E.LastFirstMIName
	,CostCenter = E.CostCenterGroupCode
	,CostCenterLine =
	(
	   ISNULL
	   (
		  (
			 SELECT TOP 1
				[Description]
			 FROM maintenance.CostCenter
			 WHERE CHARINDEX('','' + CAST(E.LineCodeId AS VARCHAR) + '','', '','' + Value + '','') > 0 AND CostCenterMappingId = 10
		  )
	   , ''''
	   )
	)
	,ManpowerClass = E.ManpowerClassificationCode
	,TimeIn =
	(
	    CASE
		    WHEN CAST(ISNULL(ETE.ActualTimeIn, ''1/1/1900'') AS DATETIME) > CAST(''1/1/1900'' AS DATETIME) THEN FORMAT(ETE.ActualTimeIn, N''MM/dd/yyyy hh:mm tt'')
		    ELSE ''''
	    END
	)
	,[TimeOut] =
	(
	    CASE
		    WHEN CAST(ISNULL(ETE.ActualTimeOut, ''1/1/1900'') AS DATETIME) > CAST(''1/1/1900'' AS DATETIME) THEN FORMAT(ETE.ActualTimeOut, N''MM/dd/yyyy hh:mm tt'')
		    ELSE ''''
	    END
	)
	,ShiftCode = ETE.Type
	,Late = ISNULL(ETE.Late, 0)
	,Undertime = ISNULL(ETE.Undertime, 0)
	,EmployeeId = ETE.ProfileId
	,ETE.ProjectProfileId
	,E.CompanyId
	,E.DepartmentName
/*FROM vwEmployeeTimeEntry ETE*/
FROM report.fnEmployeeTimeEtnry(0,0,@StartWorkDate,@EndWorkDate,0) ETE



/*LEAVE DETAILS*/
OUTER APPLY
(
    SELECT TOP 1 
	   LEAVESTATUSID = 
	   (
		  CASE WHEN ISNULL(@IgnoreLeaves,''false'') = ''true'' THEN 0 
		  ELSE 
		  (
			 CASE WHEN (ISNULL(ETE.Late,0) + ISNULL(ETE.Undertime,0)) > ISNULL(ETELeave.LeaveHours,0) THEN 0 ELSE leavereq.StatusId END
		  ) END
	   )
    FROM attendance.TimeEntryLeaveDetail ETELeave
    INNER JOIN leave.LeaveRequest leavereq 
	   ON leavereq.LeaveRequestId = ETELeave.LeaveRequestId
    WHERE ETELeave.TimeEntryHeaderDetailId = ETE.TimeEntryHeaderDetailId
)LEAVE

/*OB DETAILS*/
OUTER APPLY
(
    SELECT TOP 1 
	   OBSTATUSID = 
	   (
		  CASE WHEN ISNULL(ETE.OBTypeId,0) = 1 THEN 
		  (
			 CASE WHEN ISNULL(@IgnoreOB,''false'') = ''true'' THEN 0 
			 ELSE 0 END
		  ) END
	   )
	   ,TOSTATUSID = 
	   (
		  CASE WHEN ISNULL(ETE.OBTypeId,0) = 2 THEN 
		  (
			 CASE WHEN ISNULL(@IgnoreTimeOff,''false'') = ''true'' THEN 0 
			 ELSE 0 END
		  ) END
	   )
    FROM schedule.OfficialBusiness OB
    WHERE OB.OfficialBusinessId = ETE.OfficialBussinessId 
)OB

CROSS APPLY 
(
    SELECT TOP 1
		e.EmployeeNo
		,e.LastFirstMIName
		,e.CostCenterGroupCode
		,e.LineCodeId
		,e.ManpowerClassificationCode
		,e.CompanyId
		,e.DepartmentId
		,e.DepartmentName
	FROM dbo.fnProfileInfo(@CompanyId,@BranchId,0,@OfficeId,0,0,0,@ProjectProfileId,@CostCenterId,@DistrictId,0,0,0,@UnitId,0,@DivisionId,@GroupId,@TeamId,@LineId,1,ETE.TimeEntryHeaderDetailId) e
	WHERE e.ProfileId = ETE.ProfileId
) E
WHERE 
(
    ((ETE.Late + ETE.Undertime) > 0)
   /* AND
    (
	   CAST(ISNULL(@StartDate,''1/1/1900'') AS DATETIME) <= CAST(''1/1/1900'' AS DATETIME) OR
	   (ETE.WorkDate >= CAST(@StartDate AS DATETIME))
    )
    AND
    (
	   CAST(ISNULL(@EndDate,''1/1/1900'') AS DATETIME) <= CAST(''1/1/1900'' AS DATETIME) OR
	   (ETE.WorkDate <= CAST(@EndDate AS DATETIME))
    ) */
)
AND
(
    (ISNULL(LEAVE.LEAVESTATUSID,0) = 0)
    AND 
    (ISNULL(OB.OBSTATUSID,0) = 0)
    AND
    (ISNULL(OB.TOSTATUSID,0) = 0)
    AND
    (
	   CAST(ISNULL(@ShowLate,0) AS DECIMAL(18,2)) = CAST(0 AS DECIMAL(18,2)) OR CAST(ISNULL(ETE.Late,0) AS DECIMAL(18,2)) >= CAST(@ShowLate AS DECIMAL(18,2))
    )
    AND
    (
	   CAST(ISNULL(@ShowUndertime,0) AS DECIMAL(18,2)) = CAST(0 AS DECIMAL(18,2)) OR CAST(ISNULL(ETE.Undertime,0) AS DECIMAL(18,2)) >= CAST(@ShowUndertime AS DECIMAL(18,2))
    )
    AND
    (
	  (@EmployeeId = '''') OR (CHARINDEX('''''''' + CAST(ETE.ProfileId as VARCHAR) + '''''''',@EmployeeId) > 0)
    )
    AND
    (
	   (@DepartmentIds = '''') OR (CHARINDEX('''''''' + CAST(E.DepartmentId as VARCHAR) + '''''''',@DepartmentIds) > 0)
    )
)',NULL,1,'QUERY',1,'This report will show the lates and undertime of employees.','\TimeAndAttendance\LatesAndUndertimeReport.png',NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL)
