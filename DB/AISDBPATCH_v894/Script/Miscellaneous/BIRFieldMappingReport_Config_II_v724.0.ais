	,CompanyId = 0
FROM compben.EarningDeduction ED
INNER JOIN compben.vwRetrieveEarningDeductionWithoutHoursInCode ED2 ON ED2.EarningDeductionId = ED.EarningDeductionId
CROSS APPLY(SELECT * FROM dbo.SplitStrings_XML(replace(ED.BIRFormIds,''|'','',''),'',''))BIR
